	{"name": "trailing space", "input": "meow    ", "output": ["meow", "", false]},
	{"name": "only spaces", "input": "   ", "output": ["", "", false]},
	{"name": "leading spaces", "input": "  meow woof", "output": ["", "meow woof", false]},
